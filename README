# install over ssh
curl -sSL https://raw.github.com/reinventing-wheels/dotfiles/master/.dotfiles/.dotfiles-install | bash -s ssh

# install over https
curl -sSL https://raw.github.com/reinventing-wheels/dotfiles/master/.dotfiles/.dotfiles-install | bash

# commands
.dotfiles
.dotfiles-sync
.dotfiles-uninstall
